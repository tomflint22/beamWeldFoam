FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.metal 0
);

regions
(


    boxToCell
    {
        box (-85 0.001 -1) (85 0.003 1);
        fieldValues
        (
volScalarFieldValue alpha.metal 1.0
        );
    }



);


