{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (phase1 phase2);

phase1
{
    transportModel  Newtonian;
    nu              2.8e-5;
    rho             5000;
    cp  100;
    kappa  70.0; 
    Tsolidus    -40;
    Tliquidus   -10;
    LatentHeat  270000;
    beta    1.34e-5;
}

phase2
{
    transportModel  Newtonian;
    nu              1.185e-10;
    rho             1.185;
    cp  1000;
    kappa      1e-10;
    Tsolidus    -10.0;
    Tliquidus   -5.0;
    LatentHeat  1.0;
    beta    5.0e-12;
}

sigma            0.0025;
dsigmadT	-9.8e-05;

p0		100000.0;
Tvap 300000.0;
