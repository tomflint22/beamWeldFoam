FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState Boussinesq;
    specie          specie;
    energy          sensibleEnthalpy;
}

// ************************************************************************* //

mixture
{
    specie
    {
        nMoles          1;
        molWeight       50;
    }
    equationOfState
    {
        beta            1.2e-4;
        rho0            6100;
        T0              301.3;
    }
    thermodynamics
    {
        Cp              381;
        Hf              0;
    }
    transport
    {
        mu              2.8e-3;
        Pr              0.031;
    }
}

// ************************************************************************* //
